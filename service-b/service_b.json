[
  {
    "name": "service-b-deploy",
    "image": "seyhello/revolgy-task:service-b",
    "cpu": 512,
    "memory": 1024,
    "networkMode": "awsvpc",
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "log_revolgy_service-b",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "revolgy_service-b"
        }
    },
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ]
  }
]
