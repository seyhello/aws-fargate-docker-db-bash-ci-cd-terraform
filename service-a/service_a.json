{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "inferenceAccelerators": [],
    "containerDefinitions": [
        {
          "name": "service-a",
          "image": "seyhello/revolgy-task:service-a",
            "resourceRequirements": null,
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                  "awslogs-group": "log_revolgy_service-a",
                  "awslogs-region": "eu-central-1",
                  "awslogs-stream-prefix": "revolgy_service-a"
                }
            },
            "portMappings": [
              {
                "containerPort": 80,
                "hostPort": 80,
                "protocol" : "tcp"
              }
            ]
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512",
    "executionRoleArn": "arn:aws:iam::604725720641:role/role_revolgy_ECSTaskExecution",
    "family": "task_revolgy_service-a_release_deployed",
    "taskRoleArn": "",
    "placementConstraints": []
}
